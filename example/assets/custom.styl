.custom-json-pretty
  background: #666
  line-height 1.3
  color #66d9ef
  white-space pre-wrap;       /* Since CSS 2.1 */
  white-space -moz-pre-wrap;  /* Mozilla, since 1999 */
  white-space -pre-wrap;      /* Opera 4-6 */
  white-space -o-pre-wrap;
  word-wrap break-word

  .json-key
    color #F92672

  .json-value
    color #A6E22E

  .json-string
    color #FD971F

  .json-boolean
    color #AC81Fe